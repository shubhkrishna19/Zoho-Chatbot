{
    "version": 2,
    "functions": {
        "api/*.js": {
            "runtime": "@vercel/node@3"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/chat-widget.js",
            "dest": "/public/chat-widget.js"
        },
        {
            "src": "/test.html",
            "dest": "/public/test.html"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}