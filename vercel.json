{
    "version": 2,
    "buildCommand": "",
    "outputDirectory": "",
    "installCommand": "npm install",
    "framework": null,
    "builds": [
        {
            "src": "api/*.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/chat-widget.js",
            "dest": "/public/chat-widget.js"
        },
        {
            "src": "/test.html",
            "dest": "/public/test.html"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ],
    "env": {
        "GOOGLE_API_KEY": "@google_api_key"
    },
    "regions": [
        "bom1"
    ]
}