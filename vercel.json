{
    "version": 2,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/chat-widget.js",
            "destination": "/public/chat-widget.js"
        },
        {
            "source": "/test.html",
            "destination": "/public/test.html"
        },
        {
            "source": "/(.*)",
            "destination": "/public/$1"
        }
    ]
}